{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "types": ["@nuxt/types", "@nuxt/image", "@pinia/nuxt", "@nuxtjs/i18n"],
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": ["node_modules", ".output", "dist"]
}
