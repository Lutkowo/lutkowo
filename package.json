{
  "name": "lutkowo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run nuxt build",
    "dev": "bun run nuxt dev --open",
    "generate": "bun run nuxt generate",
    "preview": "bun run nuxt preview",
    "postinstall": "bun run nuxt prepare",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "dev:env": "bun run nuxt dev --dotenv .env",
    "build:env": "bun run nuxt build --dotenv .env",
    "deploy": "bun run generate && firebase deploy",
    "deploy:hosting": "bun run generate && firebase deploy --only hosting"
  },
  "dependencies": {
    "@nuxt/fonts": "0.11.1",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.0.2",
    "@nuxtjs/i18n": "^9.5.3",
    "@pinia/nuxt": "^0.11.0",
    "@vite-pwa/nuxt": "^0.10.0",
    "eslint": "^9.0.0",
    "firebase": "^11.6.0",
    "nuxt": "^3.16.2",
    "pinia": "^3.0.2",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "@eslint/js": "^9.1.0"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@nuxtjs/seo": "^3.0.3",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@vueuse/nuxt": "^13.1.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "firebase-tools": "^12.9.1",
    "typescript-eslint": "^7.4.0"
  }
}
