# Ogólne ustawienia zgodności i wersjonowania
engine-strict=true
save-exact=true
registry=https://registry.npmjs.org/
auto-install-peers=true

# Dodatkowe ustawienia dla lepszej stabilności
lockfile=true
fund=false
audit=false

# Optymalizacja dla Bun (Bun również interpretuje niektóre ustawienia npm)
prefer-offline=true
cache-min=3600